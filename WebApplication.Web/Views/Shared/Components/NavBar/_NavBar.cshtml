@model Users
@{
    ViewData["Title"] = "_Navbar";
}



@if (Model == null)
{
    <li><a asp-area="" asp-controller="Account" asp-action="Register" style="color:darkslategray">Register</a></li>
    <li><a asp-area="" asp-controller="Account" asp-action="Login" style="color:darkcyan">Login</a></li>
}
else
{
    <li><a asp-area="" asp-controller="Account" asp-action="Index">Welcome @Model.Username</a></li>
    <li><a asp-area="" asp-controller="Account" asp-action="Logoff">Logout</a></li>
}